---
import BaseLayout from "@layouts/BaseLayout.astro";
import CategoryButton from "@components/CategoryButton.astro";

const subcategories = [
    {
        title: "3D - Design",
        url: "/design/3d",
    },
    {
        title: "App - Design",
        url: "/design/app",
    },
    {
        title: "Logo + Icon Design",
        url: "/design/logoicon",
    },
    {
        title: "Print",
        url: "/design/print",
    },
    {
        title: "Webdesign",
        url: "/design/web",
    },
]
---
<BaseLayout title="Design">
    <ul>
        {
            subcategories.map(subcategory => <CategoryButton subcategory={subcategory}/>)
        }
    </ul>
</BaseLayout>
